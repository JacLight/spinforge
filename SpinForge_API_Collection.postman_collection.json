          }
        }
      ]
    },
    {
      "name": "üåê Sites Management",
      "item": [
        {
          "name": "List All Sites",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/sites?search=&customer={{customerId}}&type=&limit=20&offset=0",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites"],
              "query": [
                {"key": "search", "value": ""},
                {"key": "customer", "value": "{{customerId}}"},
                {"key": "type", "value": ""},
                {"key": "limit", "value": "20"},
                {"key": "offset", "value": "0"}
              ]
            }
          }
        },
        {
          "name": "Get Site Details",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/sites/{{testDomain}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites", "{{testDomain}}"]
            }
          }
        },
        {
          "name": "Create Static Site",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"domain\": \"static.example.com\",\n  \"type\": \"static\",\n  \"enabled\": true,\n  \"ssl_enabled\": false,\n  \"customerId\": \"{{customerId}}\",\n  \"aliases\": [\"www.static.example.com\"],\n  \"indexFile\": \"index.html\",\n  \"errorFile\": \"404.html\",\n  \"directoryListing\": false\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/sites",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites"]
            }
          }
        },
        {
          "name": "Create Proxy Site",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"domain\": \"proxy.example.com\",\n  \"type\": \"proxy\",\n  \"target\": \"http://backend-service:3000\",\n  \"enabled\": true,\n  \"ssl_enabled\": false,\n  \"customerId\": \"{{customerId}}\",\n  \"websocketEnabled\": true,\n  \"gzipEnabled\": true,\n  \"requestTimeout\": 60,\n  \"maxRequestSize\": 100\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/sites",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites"]
            }
          }
        },
        {
          "name": "Create Container Site",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"domain\": \"app.example.com\",\n  \"type\": \"container\",\n  \"enabled\": true,\n  \"ssl_enabled\": false,\n  \"customerId\": \"{{customerId}}\",\n  \"containerConfig\": {\n    \"image\": \"nginx:latest\",\n    \"port\": 80,\n    \"env\": [\n      {\"key\": \"NODE_ENV\", \"value\": \"production\"},\n      {\"key\": \"API_KEY\", \"value\": \"secret123\"}\n    ],\n    \"volumes\": [],\n    \"restartPolicy\": \"unless-stopped\",\n    \"memoryLimit\": \"512m\",\n    \"cpuLimit\": \"0.5\"\n  },\n  \"additionalEndpoints\": [\n    {\n      \"domain\": \"api.example.com\",\n      \"port\": 3000,\n      \"enabled\": true\n    }\n  ]\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/sites",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites"]
            }
          }
        },
        {
          "name": "Create Load Balancer Site",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"domain\": \"lb.example.com\",\n  \"type\": \"loadbalancer\",\n  \"enabled\": true,\n  \"ssl_enabled\": false,\n  \"customerId\": \"{{customerId}}\",\n  \"backends\": [\n    {\n      \"url\": \"http://backend1:3000\",\n      \"label\": \"primary\",\n      \"weight\": 2,\n      \"enabled\": true\n    },\n    {\n      \"url\": \"http://backend2:3000\",\n      \"label\": \"secondary\",\n      \"weight\": 1,\n      \"enabled\": true\n    }\n  ],\n  \"lbMethod\": \"round-robin\",\n  \"stickySessionDuration\": 0,\n  \"routingRules\": [\n    {\n      \"type\": \"cookie\",\n      \"matchType\": \"exact\",\n      \"name\": \"version\",\n      \"value\": \"beta\",\n      \"targetLabel\": \"secondary\",\n      \"priority\": 1\n    }\n  ]\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/sites",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites"]
            }
          }
        },
        {
          "name": "Create Site with Auth",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"domain\": \"protected.example.com\",\n  \"type\": \"static\",\n  \"enabled\": true,\n  \"ssl_enabled\": false,\n  \"customerId\": \"{{customerId}}\",\n  \"authConfig\": {\n    \"enabled\": true,\n    \"routes\": [\n      {\n        \"pattern\": \"/*\",\n        \"authType\": \"custom\",\n        \"unauthorizedRedirect\": \"https://login.example.com\",\n        \"customAuthConfig\": {\n          \"authUrl\": \"https://auth.example.com/validate\",\n          \"tokenParam\": \"X-Auth-Token\",\n          \"responseMappings\": [\n            {\n              \"responsePath\": \"X-Auth-Token\",\n              \"cookieName\": \"X_Auth_Token\",\n              \"envVarName\": \"X_AUTH_TOKEN\",\n              \"isToken\": true\n            },\n            {\n              \"responsePath\": \"X-Email\",\n              \"cookieName\": \"X_Email\",\n              \"envVarName\": \"X_EMAIL\"\n            }\n          ]\n        }\n      }\n    ]\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/sites",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites"]
            }
          }
        },
        {
          "name": "Update Site",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"enabled\": true,\n  \"ssl_enabled\": true,\n  \"aliases\": [\"www.{{testDomain}}\", \"api.{{testDomain}}\"],\n  \"requestTimeout\": 120,\n  \"maxRequestSize\": 200\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/sites/{{testDomain}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites", "{{testDomain}}"]
            }
          }
        },
        {
          "name": "Delete Site",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{baseUrl}}/api/sites/{{testDomain}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites", "{{testDomain}}"]
            }
          }
        },
        {
          "name": "Upload Static Files",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "zipfile",
                  "type": "file",
                  "src": "/path/to/your/website.zip"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/api/sites/{{testDomain}}/upload",
              "host": ["{{baseUrl}}"],
              "path": ["api", "sites", "{{testDomain}}", "upload"]
            }
          }
        }
      ]
    },
